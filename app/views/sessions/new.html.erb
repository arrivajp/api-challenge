<%= form_tag sessions_path, method: :post do %>
  <div class="field">
    <%= label_tag :email%>
    <%= text_field_tag :email%>
  </div>
  <div class="field">
    <%= label_tag :password%>
    <%= password_field_tag :password%>
  </div>

  <div>
    <%= submit_tag "Login"%>
  </div>
  <%end%>

  <%= link_to 'create new account', new_user_path%>
